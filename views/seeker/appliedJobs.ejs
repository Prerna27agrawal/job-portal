<%- include("../partials/header") %>


<% jobs.forEach(function(job){ 
    var bool=false;
    job.appliedBy.forEach(function(thisuser){
        if(thisuser._id.equals(currentUser._id)){
            bool=true;
        }
    });
    if(bool){ %>
        <p><%= job.name %></p>
        <p><%= job.location %></p>
        <p><%= job.experience %></p>
        <p><%= job.description %></p>
    <% }
}); %>
    


<%- include("../partials/footer") %>